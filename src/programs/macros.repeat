MACRO times (r1 r2)
    repeat r1
        repeat r2
            inc r0
        end
    end
end

MACRO plus (r1 r2)
    repeat r1
        inc r0
    end
    repeat r2
        inc r0
    end
end

MACRO plustimes (r1 r2 r3 r4)
    r5 <- plus (r1 r2)
    r6 <- plus (r3 r4)
    r0 <- times (r5 r6)
    end

r1 <- 1
r2 <- 2
r3 <- 3
r4 <- 4

r5 <- plus (r1 r2)
r6 <- plus (r3 r4)
r7 <- times (r5 r6)
